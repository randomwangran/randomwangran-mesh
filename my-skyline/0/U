FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

flowVelocity    (0 20 0);

pressure        0;

turbulentKE     0.24;

turbulentOmega  1.78;

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 20 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 20 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 20 0);
    }
    walls
    {
        type            fixedValue;
        value           uniform (0 20 0);
    }
    stlmesh
    {
        type            noSlip;
    }
}


